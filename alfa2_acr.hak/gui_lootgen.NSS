void main(int nCurrentLoot, int nLowestLevel, int nExperience, int nPCCount, int nDistribution, int nSpawnPriority) {
	SendMessageToAllDMs("Current loot = " + IntToString(nCurrentLoot));
	SendMessageToAllDMs("Lowest Level = " + IntToString(nLowestLevel));
	SendMessageToAllDMs("Experience = " + IntToString(nExperience));
	SendMessageToAllDMs("Number of PCs = " + IntToString(nPCCount));
	SendMessageToAllDMs("Distribution = " + IntToString(nDistribution));
	SendMessageToAllDMs("Spawn priority = " + IntToString(nSpawnPriority));
}
