function Get-ALFAServers
{
#.SYNOPSIS
#	Returns an array describing the IDs of production ALFA servers.
#.DESCRIPTION
#	Returns an array describing the IDs of production ALFA servers.
	return @( 3, 9, 10, 11 )
}

function Get-ALFAServersString
{
#.SYNOPSIS
#	Returns a string describing the IDs of production ALFA servers.
#.DESCRIPTION
#	Returns a string describing the IDs of production ALFA servers.
	return "(3, 9, 10, 11)"
}


